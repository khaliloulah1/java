pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') {
            steps {
                withMaven(traceability: true) {
                }
            }
        }
        stage('Integration') {
            junit 'test-results.xml'
        }



        stage('Ignored') {
            withChecks('Integration Tests') {
                junit 'yet-more-test-results.xml'
            }
        }
        stage('Deploy') {
            steps {
                withMaven(traceability: true) {
                }
            }
        }



    }}
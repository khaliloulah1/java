pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') {
            steps {
                withMaven(traceability: true) {
                }
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
                withMaven(traceability: true) {
                }
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                    withMaven(traceability: true) {
                    }
                }
            }
        }
        stage('Deliver') {
            steps {
                sh './jenkins/scripts/deliver.sh'
            }
        }



    }}